{
  "expo": {
    "name": "TVS Jugendtrainer",
    "slug": "tvs-jugendtrainer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore-icon-1024.png",
    "scheme": "tvsjugendtrainer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tekanet.jugendtrainer",
      "usesAppleSignIn": true,
      "infoPlist": {
        "CFBundleDisplayName": "TVS Jugendtrainer",
        "NSCalendarsUsageDescription": "Wir benötigen Zugriff auf deinen Kalender, um Turniertermine zu speichern.",
        "NSCalendarsFullAccessUsageDescription": "Wir benötigen vollen Zugriff auf deinen Kalender, um Turniertermine zu verwalten.",
        "NSRemindersUsageDescription": "Wir benötigen Zugriff auf Erinnerungen für Turnierbenachrichtigungen.",
        "NSContactsUsageDescription": "Wir benötigen Zugriff auf deine Kontakte, um Spieler und Trainer hinzuzufügen.",
        "NSCameraUsageDescription": "Wir benötigen Zugriff auf die Kamera, um Profilbilder und Teamfotos aufzunehmen."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tekanet.jugendtrainer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "./plugins/withJugendtrainerIosFixes",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11d91eea-fef4-4767-8ce8-8c753b86593e"
      }
    }
  }
}
